# Insert colPos 0 in main content
lib.content.main = COA
lib.content.main {
	50 < styles.content.get
	50.select.where = colPos=0
}
# Insert colPos 1 in menu content
lib.content.menu = COA
lib.content.menu {
	stdWrap.prefixComment = 2|Output of lib.content.menu
	stdWrap.outerWrap = |
	20 =< lib.menu.sub
	50 < styles.content.get
	50.select.where = colPos=1
}
# Insert colPos 2 in menu content
lib.content.sidebar = COA
lib.content.sidebar {
	50 < styles.content.get
	50.select.where = colPos=2
}
# Insert colPos 3 in feature content
lib.content.feature = COA
lib.content.feature {
	50 < styles.content.get
	50.select.where = colPos=3
}
# Insert colPos 4 in extended content
lib.content.extended = COA
lib.content.extended {
	50 < styles.content.get
	50.select.where = colPos=4
}
